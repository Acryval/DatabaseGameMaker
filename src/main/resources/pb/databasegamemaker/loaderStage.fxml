<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<VBox alignment="CENTER" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="pb.databasegamemaker.LoaderStage">
    <Label fx:id="label"/>
    <Separator/>
    <Separator/>
    <Button fx:id="bReload" text="Reload Game Engine" onAction="#reload"/>
    <Separator/>
    <Separator/>
    <Button fx:id="bDrop" text="Remove Game Engine" onAction="#dropSchema"/>
    <Separator/>
    <Separator/>
    <Separator/>
    <Button fx:id="button" onAction="#loadEngine"/>
</VBox>
